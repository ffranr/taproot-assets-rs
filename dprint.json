{
  "excludes": ["external/**"],
  "includes": ["**/*.md", "**/*.toml"],
  "markdown": {
    "textWrap": "always",
    "lineWidth": 80
  },
  "toml": {
    "lineWidth": 80
  },
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.18.0.wasm",
    "https://plugins.dprint.dev/toml-0.6.0.wasm"
  ]
}
